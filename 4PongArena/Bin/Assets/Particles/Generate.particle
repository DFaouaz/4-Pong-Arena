particle_system Generate
{ 
  material Generate
  particle_width 0.5
  particle_height 0.3
  quota 20

  emitter Point
  { 
    direction 0 0 -1
    position 0 0 -1.2
    emission_rate 120
    time_to_live 0.4
    velocity_min 1
    velocity_max 3
  }

  affector LinearForce
  { 
    force_vector 0 -5 0
  }

  affector Rotator
  { 
    rotation_range_start 0
    rotation_range_end 360
    rotation_speed_range_start -10
    rotation_speed_range_end 10
  }
}